{
  "name": "ug-locations",
  "version": "1.2.0",
  "description": "A fast, intelligent library for searching Uganda's administrative units with fuzzy matching and full hierarchy lookup",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target=bun --external=./data-optimized.json && cpy ./src/data-optimized.json ./dist/"
  },
  "keywords": [
    "uganda",
    "locations",
    "districts",
    "subcounties",
    "parishes",
    "villages",
    "hierarchy",
    "fuzzy-search",
    "africa"
  ],
  "author": "Natumanya Guy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NatumanyaGuy/ug-locations"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "cpy-cli": "^6.0.0",
    "fuse.js": "^7.1.0"
  }
}
